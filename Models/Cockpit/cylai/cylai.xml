<PropertyList>

<model>
<path>Aircraft/Kfir/Models/Cockpit/cylai/cylai.ac</path>
</model>

<animation>
<type>translate</type>
<object-name>ball</object-name>
<property>engines/engine/running</property>
<factor>-0.0005</factor>
<interpolation>
<entry><ind>-180</ind><dep>-0.001</dep></entry>
<entry><ind>0</ind><dep>-0.001</dep></entry>
<entry><ind>180</ind><dep>-0.001</dep></entry>
</interpolation>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>ball</object-name>
<property>/orientation/pitch-deg</property>
<interpolation>
<entry><ind>90</ind><dep>48.7</dep></entry>
<entry><ind>-90</ind><dep>-48.7</dep></entry>
</interpolation>
<center>
<x-m>-0.05</x-m>
<y-m> 0.00</y-m>
<z-m> 0.00</z-m>
</center>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>ball</object-name>
<property>/orientation/roll-deg</property>
<factor>1</factor>
<center>
<x-m>-0.05</x-m>
<y-m> 0.00</y-m>
<z-m> 0.00</z-m>
</center>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

</PropertyList>