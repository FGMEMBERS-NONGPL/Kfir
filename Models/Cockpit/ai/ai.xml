<PropertyList>

<model>
<path>Aircraft/Kfir/Models/Cockpit/ai/ball.ac</path>
</model>

<animation>
<type>rotate</type>
<object-name>ball</object-name>
<property>orientation/pitch-deg</property>
<interpolation>
<entry><ind>90</ind><dep>90</dep></entry>
<entry><ind>-90</ind><dep>-90</dep></entry>
</interpolation>
<center>
<x-m>-0.089</x-m>
<y-m> 0.000</y-m>
<z-m> 0.000</z-m>
</center>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>ball</object-name>
<property>orientation/roll-deg</property>
<factor>1</factor>
<center>
<x-m>-0.089</x-m>
<y-m> 0.000</y-m>
<z-m> 0.000</z-m>
</center>
<axis>
<x>1</x>
<y>0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>ball</object-name>
<property>orientation/heading-deg</property>
<factor>1</factor>
<center>
<x-m>-0.089</x-m>
<y-m> 0.000</y-m>
<z-m> 0.000</z-m>
</center>
<axis>
<x>0</x>
<y>0</y>
<z>1</z>
</axis>
</animation>

</PropertyList>