<PropertyList> 

<model>
<path>Aircraft/Kfir/Models/Cockpit/knots/knots.ac</path>
</model>

<animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>velocities/airspeed-kt</property>
  <interpolation>
    <entry>
      <ind>0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1200</ind>
      <dep>360</dep>
    </entry>
  </interpolation>   
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>