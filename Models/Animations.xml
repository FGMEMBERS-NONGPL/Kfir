<animation>
 <type>select</type>
 <object-name>reheat</object-name>
 <property>controls/engines/engine[0]/throttle/</property>
 <condition>
  <equals>
   <property>controls/engines/engine[0]/throttle/</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
